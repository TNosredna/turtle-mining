function remove(file)
	shell.run("rm",file)
end
function get(paste,file)
	remove(file)
	shell.run("pastebin","get",paste,file)
end

get("mcEpZDQe","mineinstall")
get("1dzpNV9y","digroom")
get("EmRNH0Gu","direroom")
